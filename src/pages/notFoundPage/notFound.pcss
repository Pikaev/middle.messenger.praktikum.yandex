.notFound {
  align-items: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  min-height: 100vh;

  &__code {
    font-size: 40px;
    margin-bottom: 20px;
  }
  &__text {
    font-size: 20px;
    margin-bottom: 70px;
  }
  &__link {
    font-size: 11px;
    text-decoration: none;
  }
}
